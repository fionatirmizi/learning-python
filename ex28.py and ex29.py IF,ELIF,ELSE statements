people = 20
cats = 30
dogs = 15

if people < cats:
  print("too many cats!! the world is doomed!")

if people > cats:
  print("not many cats! world is saved!")

if people < dogs:
  print("the world is drooled on")

if people > dogs:
  print("the word is dry")

dogs +=5

if people >= dogs:
  print('peple are greater than or equal to dogs')

if people <= dogs:
  print('people are less than or equal to dogs')

if people == dogs:
  print("people are dogs")


###ex29.py below:

people = 30
cars = 40
buses = 15

if cars > people:
  print('we should take the cars.')
elif cars < people:
  print("we shouldn't take cars")
else:
  print("we cant decide.")

if buses > cars:
  print("many buses")
elif buses < cars:
  print("too many cars")
else:
  print(" well we still cant decide")

